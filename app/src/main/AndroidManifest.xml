<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.au.ccf">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".global.CCFApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.ActLogin"
            android:theme="@style/AppFullScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ActLandingMember">
        </activity>
        <activity
            android:name=".activities.ActLandingProvider">
        </activity>
        <activity
            android:name=".activities.ActRequestQuote">
        </activity>
        <activity
            android:name=".activities.ActRequestQuoteConfirmation">
        </activity>
        <activity
            android:name=".activities.ActQuoteList">
        </activity>
        <activity
            android:name=".activities.ActDetailQuote">
        </activity>
    </application>

</manifest>